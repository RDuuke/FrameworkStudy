{% extends 'layout.twig' %}
{% block title "Mis articles" %}
{% block body %}

   <h1>Welcome the Blog!</h1>
    <ul class="collection">
    {% for post in posts %}
       <a class="collection-item" href="{{ path('blog.show', {slug:post.slug, id: post.id}) }}">{{ post.name }}</a>
    {%  endfor %}
    </ul>
{% endblock %}

